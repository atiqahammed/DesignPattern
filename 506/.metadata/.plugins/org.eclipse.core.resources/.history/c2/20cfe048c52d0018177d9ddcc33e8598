package flagAssignment;

import java.util.ArrayList;


public abstract class CompositShape implements Shap{

	protected ArrayList<Shap> shapes;

	public CompositShape() {
		shapes = new ArrayList<>();
	}

	@Override
	public void drow() {

		buildShap();
		for(Shap shap: shapes)
			shap.drow();
	}

	public abstract void buildShap();

}

