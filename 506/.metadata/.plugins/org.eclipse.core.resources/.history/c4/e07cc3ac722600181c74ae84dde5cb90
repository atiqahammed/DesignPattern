package Tour;

public abstract class Tour {
	protected void makeATour() {
		journeyFromCoxToDhaka();
		khoyaDaoya();
		sunButh();
		roamingAround();
		journeyFromCoxToDhaka();
	}
	
	abstract void journeyFromCoxToDhaka();
	abstract void roamingAround();
	abstract void journeyFromCoxToDhaka();
	abstract void khoyaDaoya();
	abstract void sunButh();
}
