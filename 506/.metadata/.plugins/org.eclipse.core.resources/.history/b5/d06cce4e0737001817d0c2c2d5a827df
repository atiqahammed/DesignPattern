package circle;

public class Circle {

	private int color;
	private int redious;
	private int x;
	private int y;

	public Circle(int color, int redious, int x, int y) {
		this.color = color;
		this.redious = redious;
		this.x = x;
		this.y = y;
	}

	public void drow() {
		System.out.println("color = " + color + ", x = " + x + ", y = " + y);
	}
}
