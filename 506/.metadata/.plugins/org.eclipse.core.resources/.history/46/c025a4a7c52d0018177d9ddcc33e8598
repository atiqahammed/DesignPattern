package flagAssignment;

import java.util.ArrayList;

import allShapes.IShap;


public class CompositShape implements Shape{

	protected ArrayList<Shape> shapes;

	@Override
	public void drow() {

	}


	public CompositShape() {
		shapes = new ArrayList<>();
	}

	@Override
	public void drow() {

		buildShap();
		for(IShap shap: shapes)
			shap.drow();
	}

}

