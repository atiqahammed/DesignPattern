package main;

import java.util.Random;

import circle.Circle;

public class Test {

	 
	
	public static void main(String[] args) {
		Random rand = new Random();
		for (int i = 0; i < 10; i++) {
			new Circle(rand.nextInt(1000)%7, rand.nextInt(1000), rand.nextInt(1000), rand.nextInt(1000)).drow();
		}

	}

}
