package flagAssignment;

import java.util.ArrayList;

import allShapes.IShap;


public abstract class CompositShape implements Shape{

	protected ArrayList<Shape> shapes;


	public CompositShape() {
		shapes = new ArrayList<>();
	}

	@Override
	public void drow() {

		buildShap();
		for(Shape shap: shapes)
			shap.drow();
	}

	public abstract void buildShap();

}

